{
  "name": "example",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "prebuild": "shx mkdir -p ./out && shx cp -rf ./src/index.html ./out",
    "build": "esbuild src/index.ts --bundle --minify --outfile=out/js/index.mjs",
    "clean": "shx rm -rf ./out",
    "rebuild": "npm run clean && npm run build",
    "watch": "npm run build -- --watch",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "packageManager": "pnpm@10.7.1",
  "devDependencies": {
    "@eslint/js": "^9.24.0",
    "esbuild": "^0.25.2",
    "eslint": "^9.24.0",
    "eslint-plugin-jsdoc": "^50.6.9",
    "globals": "^16.0.0",
    "prettier": "^3.5.3",
    "sass-material-design": "workspace:*",
    "shx": "^0.4.0",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.29.0"
  }
}
