{
  "name": "sass-material-design",
  "version": "1.0.0",
  "private": true,
  "description": "",
  "main": "dist/js/material.js",
  "types": "dist/types/material.d.ts",
  "scripts": {
    "prebuild": "npx shx mkdir -p ./dist && npx shx cp -ru ./src/assets ./dist",
    "build": "npx tsc",
    "watch": "npx concurrently \"npx tsc --watch\" \"npx sass ./src/sass/material.scss ./dist/material.css --watch\"",
    "clean": "npx shx rm -rf dist"
  },
  "devDependencies": {
    "@eslint/js": "^9.23.0",
    "concurrently": "^9.1.2",
    "eslint": "^9.23.0",
    "eslint-plugin-jsdoc": "^50.6.9",
    "globals": "^16.0.0",
    "sass": "^1.86.1",
    "shx": "^0.4.0",
    "stylelint": "^16.17.0",
    "stylelint-config-standard": "^37.0.0",
    "stylelint-config-standard-scss": "^14.0.0",
    "typescript": "^5.8.2",
    "typescript-eslint": "^8.29.0"
  },
  "packageManager": "pnpm@10.7.1"
}
